        #foot{
            display: flex;
            flex-direction: row;
            justify-content: space-evenly;
        }
        #foot a{
            color: black;
            text-decoration: none;
            font-size: 18px;
        }
        #foot a:hover{
            text-decoration: underline;
        }
    </style>
